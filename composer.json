{
    "name": "simoneddy/framework",
    "description": "Simple framework I work on to keep the PHP muscles fit. Almost never production ready.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
       "bin-dir": "bin"
    },
    "autoload": {
       "psr-4": {
          "Eddy\\Framework\\": "framework",
          "App\\": "src"
       },
       "files": [
           "framework/functions.php"
       ]
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2"
    },
    "require": {
        "laminas/laminas-diactoros": "^2.6",
        "laminas/laminas-httphandlerrunner": "^1.4",
        "symfony/console": "^5.2",
        "pimple/pimple": "^3.4",
        "vlucas/phpdotenv": "^5.3",
        "symfony/process": "^5.2",
        "symfony/filesystem": "^5.2",
        "doctrine/cache": "^2.0",
        "react/event-loop": "^1.1.1",
        "react/stream": "^1.1.1",
        "react/promise": "^2.8.0",
        "react/socket": "^1.6.0",
        "react/dns": "^1.5.0",
        "react/http": "^1.3.0",
        "react/cache": "^1.1.1",
        "nikic/fast-route": "^1.3",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "phpspec/phpspec": "^7.0",
        "simoneddy/coder": "dev-master",
        "illuminate/database": "^8.44",
        "illuminate/contracts": "^8.44",
        "doctrine/dbal": "^3.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/simonceddy/coder"
        }
    ]
}
