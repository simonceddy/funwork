{
    "name": "simoneddy/framework-installer",
    "description": "installer for my silly framework cos why not",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
